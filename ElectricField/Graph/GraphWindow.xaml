<Window x:Class="ElectricField.Graph.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0" Title="Graph Window" Height="370" Width="600">
    <Grid>
        <d3:ChartPlotter Name="plotter" Margin="0,0,22,0">
            <d3:ChartPlotter.HorizontalAxis>
                <d3:HorizontalIntegerAxis Name="timeAxis"/>
            </d3:ChartPlotter.HorizontalAxis>
            <d3:ChartPlotter.VerticalAxis>
                <d3:VerticalIntegerAxis Name="countAxis"/>
            </d3:ChartPlotter.VerticalAxis>

            <d3:Header FontFamily="Arial" Content="Chart" Name="header_PlotterName" />
            <d3:VerticalAxisTitle Name="verticalAxisName" FontFamily="Arial" Content="vert"/>
            <d3:HorizontalAxisTitle  Name="HorizontalAxisName" FontFamily="Arial" Content="Horiz"/>
        </d3:ChartPlotter>
    </Grid>
</Window>
